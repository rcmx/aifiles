{
    "mcpServers": {
        "context7": {
            "type": "stdio",
            "command": "npx",
            "args": ["@upstash/context7-mcp"],
            "env" : {}
        },
        "playwright": {
            "type": "stdio",
            "command": "npx",
            "args": ["@playwright/mcp@latest"],
            "env" : {}
        },
        "postgres": {
            "type": "stdio",
            "command": "docker",
            "args": [
                "run",
                "-i",
                "--rm",
                "-e",
                "POSTGRES_URL",
                "mcp/postgres",
                "${POSTGRES_URL}"
            ]
        }
    },
    "servers": {
        "context7": {
            "command": "npx",
            "args": ["@upstash/context7-mcp"]
        },
        "playwright": {
            "command": "npx",
            "args": ["@playwright/mcp@latest"]
        },
        "postgres": {
            "command": "docker",
            "args": [
                "run",
                "-i",
                "--rm",
                "-e",
                "POSTGRES_URL",
                "mcp/postgres",
                "${POSTGRES_URL}"
            ]
        }
    }
}

